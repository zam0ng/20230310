<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link rel="stylesheet" href="/css/main.css">
</head>
<body>
    <div class="title-name">
        MY PAGE
    </div>
    <a class ="list-move" href="/list">⬅</a>

    <div class="board-main">
        
    <table>
        <tr>
            <th class="list-num">글번호</th>
            <th class="list-title">제목</th>
            <th class="list-like">좋아요</th>
            <th class="list-writer">글쓴이</th>
        </tr>
    <% data.forEach((el,index) => { %>
        <tr>
            <td><%= el.id %></td>
            <td> <a href="/detail/<%= el.id %>"> <%= el.title %> </a></td>
            <td><%= el.likey %></td>
            <td><%= el.writer %></td>
        </tr>
    <% }); %>
    </table>
    </div>

</body>

<script>
let list = document.querySelector(".list");
let windows = document.querySelector(".window");

list.onclick = ()=>{
    console.log("클리되니?");
    windows.classList.toggle("active");
}
    
</script>
</html>